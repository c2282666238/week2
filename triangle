package test;

import java.util.Scanner;

public class test {
	public static void main(String[]a) {
		System.out.println("输入N行倒三角");
	Scanner scan=new Scanner(System.in);
	int N=scan.nextInt();
for(int i=1;i<=N;i++){
	for(int j=1;j<=i;j++){
		System.out.print(" ");}
	for(int j=1;j<=N-i;j++){
	System.out.print("* ");
	}
	System.out.println();
	
	}
}
}
